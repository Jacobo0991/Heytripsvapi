{
    "version": 2,
    "builds": [
        {
            "src": "/bin/www.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/bin/(.*)",
            "dest": "/bin/www.js"
        }
    ]
}